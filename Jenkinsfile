					echo 'Create the World'
            }
		stage('Coding') {
				steps {
					echo 'Coding the World'
            }
		stage('Testing') {
				steps {
					echo 'Testing the World'
            }
			}
		stage('Release') {
				steps {
					echo 'Releasing the World'
            }
			}
		stage('Support') {
				steps {
					echo 'Supporting the World'
            }
        }
    }
}
